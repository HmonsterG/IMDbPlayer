{
	"manifest_version": 3,
	"name": "IMDb Player",
	"description": "Play movies and TV shows from IMDb easily with 2Embed",
	"version": "1.1",
	"icons": {
		"16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
	},
	"content_scripts":[
    {
      "matches":[
        "https://www.imdb.com/title/*"
      ],
      "js": ["content.js"]
    }
  ],
	"action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"activeTab",
		"storage"
	]
}
